{
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "build:sass": "sass css/style.scss css/style.css",
    "build:css": "postcss css/style.css -o css/final.css",
    "build": "npm run build:sass && npm run build:css",
    "watch": "sass --watch css/style.scss css/style.css"
  }
}